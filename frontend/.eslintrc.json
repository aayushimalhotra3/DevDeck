{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prefer-const": "error",
    "no-var": "error",
    "prettier/prettier": "error",
    "react/no-unescaped-entities": "off",
    "react-hooks/exhaustive-deps": "warn",
    "@next/next/no-img-element": "warn"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  }
}
